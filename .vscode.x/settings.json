{
  "editor.tokenColorCustomizations": {
    "comments": "#00000052",
    "keywords": "#29ffb8",
    "variables": "#d9ff00",
    "textMateRules": [
      {
        "scope": "comment",
        "settings": {
          "foreground": "#00000052",
          "fontStyle": "italic"
        }
      },
      {
        "scope": "variable.other.readwrite",
        "settings": {
          "foreground": "#f700ff",
          "fontStyle": "bold"
        }
      },
      {
        "scope": "entity.name.function",
        "settings": {
          "foreground": "#ec304f",
          "fontStyle": "bold "
        }
      },
      {
        "scope": "entity.other",
        "settings": {
          "foreground": "#307bec",
          "fontStyle": "bold "
        }
      }
    ],
    "numbers": "#ffffff",
    "strings": "#808080",
    "functions": "#ff2600",
    "types": "#f3f222"
  },

  "color-highlight.enable": true,
  "workbench.colorCustomizations": {
    "editor.background": "#303030",
    "terminal.background": "#000",

    "sideBar.background": "#fff",
    "sideBar.foreground": "#000000",
    "sideBar.dropBackground": "#ff0000",
    "sideBarTitle.foreground": "#000000",

    "sideBar.border": "#2a7200",

    "sideBarSectionHeader.background": "#dfdf8f",
    "sideBarSectionHeader.foreground": "#000000",
    "sideBarSectionHeader.border": "#959c2a",

    "tab.hoverBackground": "#222",
    "tab.activeBackground": "#959c2a",
    "tab.activeForeground": "#000",
    "tab.hoverBorder": "#ff0000",
    "tab.hoverForeground": "#ff0000",
    "tab.inactiveBackground": "#444",

    "activityBar.activeBackground": "#009b5a",
    "activityBar.inactiveForeground": "#000",
    "activityBar.activeBorder": "#f0f330",
    "activityBarBadge.foreground": "#fff",
    "activityBarBadge.background": "#268547",
    "activityBar.background": "#eee",
    "activityBar.foreground": "#000"
  }
}
